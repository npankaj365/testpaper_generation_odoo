<odoo>
    <data>
        <record model="ir.ui.view" id="domain_form_view">
            <field name="name">domain.form</field>
            <field name="model">assessment.domain</field>
            <field name="arch" type="xml">
                <form string="Domain Form">
                    <notebook>
                        <page string="Domain">
                            <group>
                                <field name="name"/>
                                <field name="description"/>
                                <field name="enabled"/>
                            </group>
                        </page>
                        <page string="Sub Domain">
                            <field name="subdomain"/>
                        </page>
                    </notebook>
                </form>
            </field>
        </record>
    </data>
</odoo>